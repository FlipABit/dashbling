{
  "name": "@dashbling/build-support",
  "version": "0.3.0",
  "author": "Pascal Widdershoven",
  "description": "Hackable React based dashboards for developers",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pascalw/dashbling.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/tsc -p .",
    "prepare": "yarn run --silent build && cp ../../README.md .",
    "test": "yarn run --silent build && jest",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@babel/core": "7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "7.4.4",
    "@babel/preset-env": "7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@dashbling/core": "^0.3.0",
    "autoprefixer": "^9.5.1",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^2.0.2",
    "css-loader": "^2.1.1",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^3.2.0",
    "lerna": "^3.13.4",
    "node-sass": "^4.7.2",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.9.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.31.0",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3"
  },
  "devDependencies": {
    "@types/jest": "24.0.12",
    "@types/node": "^12.0.0",
    "jest": "24.8.0",
    "ts-jest": "^24",
    "ts-node": "^8.1.0",
    "typescript": "^3.3.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(@dashbling)/)"
    ],
    "testRegex": "((\\.|/)test)\\.(js|ts)$",
    "moduleFileExtensions": [
      "ts",
      "js",
      "json"
    ]
  },
  "engines": {
    "node": ">= 10"
  }
}
